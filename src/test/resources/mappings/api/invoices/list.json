{
  "request": {
    "method": "GET",
    "url": "/api/invoices",
    "headers": {
      "Authorization": {
        "contains": "HMAC"
      },
      "Content-Type": {
        "equalTo": "application/json"
      },
      "User-Agent": {
        "contains": "Cryptopay Java"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "api/invoices/list.json"
  }
}
